# WebSocket RPC URL for Polygon Mainnet (required)
# Example: wss://polygon-mainnet.g.alchemy.com/v2/YOUR_API_KEY
RPC_URL_WSS=

# HTTP RPC URL for Polygon Mainnet (optional, falls back to default)
# Example: https://polygon-mainnet.g.alchemy.com/v2/YOUR_API_KEY
RPC_URL_HTTP=

# Private key for wallet (must start with 0x)
PRIVATE_KEY=

# Polymarket CLOB API credentials
POLYMARKET_API_KEY=
POLYMARKET_SECRET=
POLYMARKET_PASSPHRASE=

# Gas price multiplier (optional, default: 1.1 for 10% premium)
GAS_PRICE_MULTIPLIER=1.1
